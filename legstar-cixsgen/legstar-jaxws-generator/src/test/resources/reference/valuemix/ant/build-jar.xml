<?xml version="1.0" encoding="UTF-8"?>
<!-- =================================================================================
     Create an archive for an adapter.
  -->
<project basedir=".." default="create-jar" name="build-jar">
    <target name="create-jar">
        <mkdir dir="target\src\gen\target"/>
        <jar destfile="target\src\gen\target/cixs-valuemix.jar">
            <!-- Generated JAX-WS implementation binaries -->
            <fileset dir="target\src\gen\target\classes"
                includes="com/legstar/test/cixs/valuemix/*.class"/>
            <!-- JAXB binaries -->
            <fileset dir="target\classes"
                includes="com/legstar/test/coxb/valuemix/*.class"/>
            <!-- Binding binaries -->
            <fileset dir="target\classes"
                includes="com/legstar/test/coxb/valuemix/bind/*.class"/>
        </jar>
    </target>
</project>